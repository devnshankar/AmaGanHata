{
  "expo": {
    "name": "AmaGanHata",
    "slug": "AmaGanHata",
    "version": "1.0.0",
    "scheme": "amaganhata",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-image-picker"],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pdfnotes.AmaGanHata"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "948680ec-f36a-49f3-bf95-c3baa0d97c56"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/948680ec-f36a-49f3-bf95-c3baa0d97c56"
    }
  }
}
